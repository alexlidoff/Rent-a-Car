<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <display-name>user client logged</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>userLogged</from-outcome>
            <if>#{userBean.userClient}</if>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>user staff logged</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>userLogged</from-outcome>
            <if>#{userBean.userStaff}</if>
            <to-view-id>/content/staff/applicationsListStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>closeClientProfileForm</display-name>
        <from-view-id>/content/staff/clientProfile.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>closeClientProfileForm</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>*</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openClientProfileForm</from-outcome>
            <to-view-id>/content/staff/clientProfile.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>logout</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>goHomePage</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>goHomePage</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>viewCarsForRent</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>viewCarsForRent</from-outcome>
            <to-view-id>/content/client/carsChoice.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openApplicationForm user logged</display-name>
        <from-view-id>/content/client/carsChoice.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openApplicationForm</from-outcome>
            <if>#{userBean.userLogged}</if>
            <to-view-id>/content/client/applicationClient.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openApplicationForm user not logged</display-name>
        <from-view-id>/content/client/carsChoice.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openApplicationForm</from-outcome>
            <if>#{not userBean.userLogged}</if>
            <to-view-id>/content/client/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openApplicationsListClient</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openApplicationsListClient</from-outcome>
            <to-view-id>/content/client/applicationsListClient.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openPaymentSystem</display-name>
        <from-view-id>/content/client/applicationsListClient.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openPaymentSystem</from-outcome>
            <to-view-id>/content/client/paymentSystem.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>leavePayment</display-name>
        <from-view-id>/content/client/paymentSystem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>leavePayment</from-outcome>
            <to-view-id>/content/client/applicationsListClient.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openVehiclesStaff</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openVehiclesStaff</from-outcome>
            <to-view-id>/content/staff/vehiclesListStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openVehicleForm</display-name>
        <from-view-id>/content/staff/vehiclesListStaff.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openVehicleForm</from-outcome>
            <to-view-id>/content/staff/vehicleFormStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openVehicleTypesStaff</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openVehicleTypesStaff</from-outcome>
            <to-view-id>/content/staff/vehicleTypesListStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openApplicationsStaff</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openApplicationsStaff</from-outcome>
            <to-view-id>/content/staff/applicationsListStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openApplicationFormStaff</display-name>
        <from-view-id>/content/staff/applicationsListStaff.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openApplicationFormStaff</from-outcome>
            <to-view-id>/content/staff/applicationFormStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openClientsStaff</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openClientsStaff</from-outcome>
            <to-view-id>/content/staff/clientsListStaff.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openClientFormStaff</display-name>
        <from-view-id>/content/staff/clientsListStaff.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openClientFormStaff</from-outcome>
            <to-view-id>/content/staff/clientProfile.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openStaff</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openStaff</from-outcome>
            <to-view-id>/content/staff/staffList.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openStaffForm</display-name>
        <from-view-id>/content/staff/staffList.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>openStaffForm</from-outcome>
            <to-view-id>/content/staff/staffForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>contactUs</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>contactUs</from-outcome>
            <to-view-id>/content/client/contactInformation.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openReportCarsByOptions</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openReportCarsByOptions</from-outcome>
            <to-view-id>/content/staff/reportCarsByOptions.xhtml</to-view-id>
            <redirect include-view-params="true">
                <view-param>
                    <name>reportId</name>
                    <value>1</value>
                </view-param>
            </redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <display-name>openReportCarsByFeatures</display-name>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>openReportCarsByFeatures</from-outcome>
            <to-view-id>/content/staff/reportCarsByFeatures.xhtml</to-view-id>
            <redirect include-view-params="true">
                <view-param>
                    <name>reportId</name>
                    <value>2</value>
                </view-param>
            </redirect>
        </navigation-case>
    </navigation-rule>
    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>uk</supported-locale>
            <supported-locale>ru</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>localization.texts.texts</base-name>
            <var>texts</var>
        </resource-bundle>
    </application>
</faces-config>
